<div class="jumbotron">
  <h2>Welcome to Flourish</h2>
</div>

<div>
  <%= form_for @user, user_path(@conn, :create), fn f -> %>
    <label>
      Email: <%= text_input f, :email %>
    </label>
    <label>
      Password: <%= text_input f, :password %>
    </label>
    <%= submit "Log In" %>
  <% end %>
</div>
